<body class="color-dark">
    <!-- Google Tag Manager (noscript) -->
    <% include views/googleTagManager.ejs %>
    <!-- End Google Tag Manager (noscript) -->

    <%- include ('partials/mobileComingSoon.ejs')%>
    <div class="homeWrap">
    <%- include ('partials/newMessageModal.ejs') %>
    <%- include ('partials/sideBar.ejs') %>
    <%- include ('partials/horizontalHeader.ejs')%>
    <%- include ('partials/newMessageButton.ejs')%>
    <h1 class="tutorTotal  badge badge-primary"><%=tutorCount%> Tutors Available </h1>
    <div class="tutorResultsContent "> 
        <%if(noSearch){%>
        <div class="text-light findTutorTitle">
            <h3 class="findTutorSubTitle">Find a tutor that's been in your shoes</h3>
        </div>
        <%}else{%>
        <div class="text-light findTutorTitle">
                <h3 class="findTutorSubTitle"><%=subjectName%></h3>
        </div>

        <%}%>

        <div id="connectionSearchBarContainer">
            <div id="searchBarWrapper">
                <div id="searchBar" class="active-cyan-3 active-cyan-4 mb-4">
                    <form method="POST" autocomplete="off" class="searchCourse" action="searchCourse">
                        <div class="input-container1Light">
                            <button class="searchBtn"><svg class="text-light" width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-search" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                                <path fill-rule="evenodd" d="M10.442 10.442a1 1 0 0 1 1.415 0l3.85 3.85a1 1 0 0 1-1.414 1.415l-3.85-3.85a1 1 0 0 1 0-1.415z"/>
                                <path fill-rule="evenodd" d="M6.5 12a5.5 5.5 0 1 0 0-11 5.5 5.5 0 0 0 0 11zM13 6.5a6.5 6.5 0 1 1-13 0 6.5 6.5 0 0 1 13 0z"/>
                              </svg></button>
                              <%if(course != ""){%>
                                <input id="searchInput" name="course" class=" input-field " value="<%=course%>" placeholder="Search ie MATH 1100 or College Algebra"/>
                            <%}else{%>
                                <input id="searchInput" name="course" class=" input-field " placeholder="Search ie MATH 1100 or College Algebra"/>
                            <%}%>
                           
                            </div>
                    </form>
                    <div class="findTutor-autocomplete">
                        <p class="autocomplete-status">Search Courses</p>
                    </div>
                        </div>
                       
                    </div>
                  
                </div>
                <% if(image!= ""){%>
        <!-- Insert ejs tag within src below -->
        <div class="noTutorFound">
            <h2>Oh No! No tutors for this course. <br> Request one</h2>
            <img src="/assets/img/undraw_phone_call_grmk.svg"/>
            <div class="buttonFindTutor">
                <a href="/home" class="btn btn-primary tutor-btn">Request a Tutor</a>
            </div>
        </div>
        <%}%>
                
        <div id="course-container">
         <%if(noSearch){%>
        <div class="noSearch">
            <div class="row">
                        <img class="paperImg" src="/assets/img/Paper_help_3.svg" />
                <div class="paperText">
                    <h1>Need Help...</h1>
                    <h2>Paper?</h2>
                    <h2>Test?</h2>
                    <h2>Homework?</h2>
                    <h2>Discussion Post?</h2>
                    <div class="buttonFindTutor">
                        <a href="/home" class="btn btn-primary tutor-btn">Request a Tutor</a>
                    </div>
                </div>

            </div>
        </div>
        <% }%>
        <h3 class="resultSubject"></h3>
        <%if(subjectName){%>
        <%for(x in subject){%>
        <div class='tutorBlock tutorBlockDark'>
            <ul class='tutorBlockHeader'>
                <li><img src='<%=subject[x].img%>' alt='Profile Pic' class='profile-pic' />
                    <h1 class='connectionName text-light'></h1>
                </li>
                <li class='starRating'>
                    <span><svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-star-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
