<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.10.2/fullcalendar.min.css"  />
    <link rel="stylesheet" href="../assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="../assets/css/all.css" >
    <link rel="stylesheet" href="../assets/css/calendar.css">
    
 
    <title>Calendar | Degreeme</title>

    <!-- Google Tag Manager -->
    <script type="text/javascript" src="assets/js/googleTagManager.js"></script>
    <!-- End Google Tag Manager -->
    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js" ></script>
    <script src="../assets/js/popper.min.js"></script>
    <script type="text/javascript" src="../assets/js/bootstrap.min.js"></script>
    <script src='https://cdn.jsdelivr.net/npm/moment@2.27.0/min/moment.min.js'></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.10.2/fullcalendar.min.js" ></script>

</head>
<body>
        <!-- Google Tag Manager (noscript) -->
        <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PTK9R96"
          height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
          <!-- End Google Tag Manager (noscript) -->
    <script src="../assets/js/calendar.js"></script>

    <div id='calendar-container'>
        <div class="fc-toolbar">
            <div class="fc-left">
              <div class="fc-button-group">
                <button class="fc-prev-button fc-button fc-state-default fc-corner-left" 
                 type="button">
                  <span class="fc-icon fc-icon-left-single-arrow"></span>
                </button>
                <button class="fc-next-button fc-button fc-state-default fc-corner- 
                right" type="button">
                   <span class="fc-icon fc-icon-right-single-arrow"></span>
                </button>
              </div>
              <button class="fc-today-button fc-button fc-state-default fc-corner-left 
                 fc-corner-right fc-state-disabled">Today</button>
           </div>
           <div class="fc-right">
              <div class="fc-button-group">
                  
                <select class="form-control" id="my-select">
                   <option class="fc-agendaMonth-button fc-button fc-state-default fc- 
                     corner-right" value="month">Month</option>
                   <option class="fc-agendaWeek-button fc-button fc-state- 
                    default" value="agendaWeek">Week</option>
                    <option class="fc-agendaDay-button fc-button fc-state- 
                    default" value="agendaDay">Day</option>
                </select>
              </div>
            </div>
           <div class="fc-center">
             <h2></h2>
           </div>
           <div class="fc-clear"></div>
          </div>
        <div id='calendar'>
    
        </div>

        <a href="javascript:void(0)" id="addEventButton" class="float">
            <i class="fa fa-plus my-float"></i>
            </a>
      </div>
      <div id="fullCalModal" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span> <span class="sr-only">close</span></button>
                    <h4 id="modalTitle" class="modal-title"></h4>
                </div>
                <div id="modalBody" class="modal-body"></div>
                <div class="clearfix"></div>
                <div id="modal-footer" class="modal-footer">
                    <button type="button" class="btn btn-light" data-dismiss="modal">Close</button>
                    
                </div>
            </div>
        </div>
    </div>
    <div id="addEventModal" class="modal fade">
      <div class="modal-dialog">
          <div class="modal-content">
              <div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span> <span class="sr-only">close</span></button>
                  <h4 id="addEventTitle" class="modal-title"></h4>
              </div>
              <div id="addEventBody" class="modal-body"></div>
              <div class="clearfix"></div>
              <div class="modal-footer">
                <button type="button" class="btn btn-light" data-dismiss="modal">Close</button>
              </div>
          </div>
      </div>
  </div>
</body>
</html>